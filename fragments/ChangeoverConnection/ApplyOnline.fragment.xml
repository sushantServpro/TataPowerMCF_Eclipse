<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel >
		<content>
		    <Panel id="pnlx_06">
				<html:h5>Apply Online</html:h5> 
				<html:div class="apply">
					<html:div class="row">
						<html:div class=" col-md-2 tata1">
							<html:image src="images/tata_logo_white.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
						<html:div class=" col-md-1 pull-right tata2">
							<html:image src="images/tata_emblem_white.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
					</html:div>
				</html:div>
				<html:h6 class="indicate">*Indicates Mandatory Fields</html:h6>
				<html:div class="detailrequire">
					<html:div class="detailspremises">
						<html:h5 class="indicate2">A. DETAILS OF APPLICANT(S) AND PREMISES</html:h5>
						<html:hr/>
					</html:div>
					<html:div class="boxpad">
					    <html:div class="row">
							<html:div class="col-md-2">
								<Label text="Registration No." labelFor="IdRegNo1Change"/>
							</html:div>	
							<html:div class="col-md-4">	
								<Input id="IdRegNo1Change" type="Text" class="sapUiSmallMarginBottom"
								placeholder="Enter registration no. if you have already saved in draft."
								tooltip="Enter registration no. if you have already saved in draft."/>
						    </html:div>	
							<html:div class="col-md-3">  
						       	<Button text="GO" press="onGetDraftData" class="switchcenter" ariaDescribedBy="defaultButtonDescription genericButtonDescription" />
							</html:div>
						</html:div>	
						<html:div class="row ">
							<html:div class="col-md-6">
							    <Label text="Name (As per Bill)" labelFor="nameAsBillIn"/>
								<Input id="nameAsBillInChange" required="true" type="Text" value="{ChangeOverModel>/NameBill}" class="sapUiSmallMarginBottom" placeholder="Full Name"/>
							</html:div>
							<html:div class="col-md-6">
							    <Label text="Applicant's Name" labelFor="ApplNameInput"/>
								<Input id="ApplNameInput" type="Text" value="{ChangeOverModel>/ApplName}" class="sapUiSmallMarginBottom" placeholder="Applicant's Name"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Pancard" required="true" />
								<Input id="panInputChange" type="Text" value="{ChangeOverModel>/PanNumber}" class="sapUiSmallMarginBottom" placeholder="Pancard No."/>
							</html:div>
							<html:div class="col-md-6 pancard">
								<html:h6>Pancard Number is required for TDS Deduction</html:h6>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Mobile number" required="true" />
								<Input id="mobInputChange" type="Text" value="{ChangeOverModel>/MobileNo}" class="sapUiSmallMarginBottom" placeholder="Mobile number"/>
							</html:div>
							<html:div class="col-md-6">
								<Label text="Email ID" required="true" />
								<Input id="mailInputChange" type="Text" value="{ChangeOverModel>/EmailId}" class="sapUiSmallMarginBottom" placeholder="Email ID"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-xs-12 emailmobile">
								<html:h6>Email ID and Mobile No. are required for Speedy Communication</html:h6>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-xs-12 addresspremises">
								<html:h5>Address Premises at Which Supply is required
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row buildflat">
							<html:div class="col-md-4">
								<Label text="Flat/Gala/shop No/ office No" />
								<Input id="flatInputChange" required="true" value="{ChangeOverModel>/PremiseFlatNo}" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Flat/Gala/shop No/ office No"/>
							</html:div>
							<html:div class="col-md-8">
								<Label text="Building Name/Plot No" />
								<Input id="buildInputChange" type="Text" value="{ChangeOverModel>/PremisePlotNo}" class="sapUiSmallMarginBottom" placeholder="Enter Building Name/Plot No"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Society" />
								<Input id="societyInputChange" value="{ChangeOverModel>/PremiseSociety}" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Society"/>
							</html:div>
							<html:div class="col-md-6">
								<Label text="Lane/Street" />
								<Input id="LaneStreetId" showSuggestion="true" showValueHelp="true"
									     valueHelpRequest="onVHLaneStreet1"
									     suggestionItems="{path:'LaneStreet>/results'}" 
									     value="{ChangeOverModel>/PremiseStreet}" >
									<suggestionItems>
										<core:Item text="{LaneStreet>Street}" key="{LaneStreet>StrtCode}"/>
									</suggestionItems>
								</Input>
								<!--<Input id="laneInput" value="{ChangeOverModel>/Street}" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Lane/Street"/>-->
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Suburban" />
								<Input id="SubUrbIdCO" showSuggestion="true" showValueHelp="true"
									     valueHelpRequest="onVHSubUrb1"
									     suggestionItems="{path:'SubUrban>/results'}" 
									     value="{ChangeOverModel>/PremiseLocality}" >
									<suggestionItems>
										<core:Item text="{SubUrban>CityPart}" key="{SubUrban>CityPart}"/>
									</suggestionItems>
								</Input>
								<!--<Input id="subuInput" type="Text" value="{ChangeOverModel>/Suburb}" class="sapUiSmallMarginBottom" placeholder="Enter Suburban"/>-->
							</html:div>
							<html:div class="col-md-6">
								<Label text="City" />
								<Input id="IdCityValHlp" showSuggestion="true" showValueHelp="true"
									     valueHelpRequest="onCityValHelp"
									     suggestionItems="{path:'SubUrban>/results'}" 
									     value="{ChangeOverModel>/PremiseCity}" >
									<suggestionItems>
										<core:Item text="{SubUrban>CityName}" key="{SubUrban>CityPart}"/>
									</suggestionItems>
								</Input>
								<!--<Input id="cityInput" required="true" type="Text" value="{ChangeOverModel>/City1}" class="sapUiSmallMarginBottom" placeholder="Enter City"/>-->
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Postal Code"  required="true"/>
								<Input id="IDPostalCodeVH" showSuggestion="true" showValueHelp="true"
									     valueHelpRequest="onPostalCodeHelp"
									     suggestionItems="{path:'PostalCode>/results'}" 
									     value="{ChangeOverModel>/PremisePostalCode}" >
									<suggestionItems>
										<core:Item text="{PostalCode>PostCode}" key="{PostalCode>PostCode}"/>
									</suggestionItems>
								</Input>
								<!--<Input id="postInput" type="Number" value="{ChangeOverModel>/PostCode1}" class="sapUiSmallMarginBottom" placeholder="Enter Postal Code"/>-->
							</html:div>
							<html:div class="col-md-6">
								<Label text="Landmark" />
								<Input id="landInputChange" required="true" value="{ChangeOverModel>/PremiseLandmark}" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Landmark"/>
							</html:div>
						</html:div>
						<html:div class="row typepremises">
							<html:div class="col-xs-12">
								<html:h5>type of premises</html:h5>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-2">
								<RadioButton id="RB11Change" groupName="GroupA" text="Owned" selected="true"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="RB22Change" groupName="GroupA" text="Rented"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="RB33Change" groupName="GroupA" text="Lease"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="RB44Change" groupName="GroupA" text="Other (Specify)"/>
							</html:div>
						</html:div>
						<html:div class="row premises">
							<html:div class="col-xs-12">
								<html:h5>In Case of Not Owned by Applicant(s)
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row largebox">
							<html:div class="col-xs-12">
								<Label text="Name of the Owner and full Address "  width="100%"/>
								<TextArea id="OwenerNameChange" value="{ChangeOverModel>/NameOwner}" placeholder="Enter Name of the Owner and full Address" growing="true" growingMaxLines="7" width="100%"/>
								<CheckBox id="IdSameAsAboveChange" text="Same as above" selected="false"/>
							</html:div>
						</html:div>
						<html:div class="row coresponding">
							<html:div class="col-xs-12">
								<html:h5>Coresponding Address
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row buildflat">
							<html:div class="col-md-4">
								<Label text="Flat/Gala/shop No/ office No" />
								<Input id="aflatInputChange" value="{ChangeOverModel>/CorrFlatNo}" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Flat/Gala/shop No/ office No"/>
							</html:div>
							<html:div class="col-md-8">
								<Label text="Building Name/Plot No" />
								<Input id="abuildInputChange" type="Text" value="{ChangeOverModel>/CorrPlotNo}" class="sapUiSmallMarginBottom" placeholder="Enter Building Name/Plot No"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Society" />
								<Input id="asocietyInputChange" type="Text" value="{ChangeOverModel>/CorrSociety}" class="sapUiSmallMarginBottom" placeholder="Enter Society"/>
							</html:div>
							<html:div class="col-md-6">
								<Label text="Lane/Street" />
								<!--<Input id="alaneInputChange" showSuggestion="true" showValueHelp="true"-->
								<!--	     valueHelpRequest="onVHLaneStreet1"-->
								<!--	     suggestionItems="{path:'LaneStreet>/results'}" -->
								<!--	     value="{ChangeOverModel>/CorrStreet}" >-->
								<!--	<suggestionItems>-->
								<!--		<core:Item text="{LaneStreet>Street}" key="{LaneStreet>StrtCode}"/>-->
								<!--	</suggestionItems>-->
								<!--</Input>-->
								<Input id="alaneInputChange" required="true" value="{ChangeOverModel>/CorrStreet}" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Lane/Street"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Suburban" />
								<!--<Input id="asubuInputChange" showSuggestion="true" showValueHelp="true"-->
								<!--	     valueHelpRequest="onVHSubUrb1"-->
								<!--	     suggestionItems="{path:'SubUrban>/results'}" -->
								<!--	     value="{ChangeOverModel>/CorrLocality}" >-->
								<!--	<suggestionItems>-->
								<!--		<core:Item text="{SubUrban>CityPart}" key="{SubUrban>CityPart}"/>-->
								<!--	</suggestionItems>-->
								<!--</Input>-->
								<Input id="asubuInputChange" type="Text" value="{ChangeOverModel>/CorrLocality}" class="sapUiSmallMarginBottom" placeholder="Enter Suburban"/>
							</html:div>
							<html:div class="col-md-6">
								<Label text="City" />
								<!--<Input id="acityInpuChanget" showSuggestion="true" showValueHelp="true"-->
								<!--	     valueHelpRequest="onCityValHelp"-->
								<!--	     suggestionItems="{path:'SubUrban>/results'}" -->
								<!--	     value="{ChangeOverModel>/CorrCity}" >-->
								<!--	<suggestionItems>-->
								<!--		<core:Item text="{SubUrban>CityName}" key="{SubUrban>CityPart}"/>-->
								<!--	</suggestionItems>-->
								<!--</Input>-->
								<Input id="acityInpuChanget" required="true" value="{ChangeOverModel>/CorrCity}" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter City"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Postal Code"  required="true"/>
								<!--<Input id="apostInputChange" showSuggestion="true" showValueHelp="true"-->
								<!--	     valueHelpRequest="onPostalCodeHelp"-->
								<!--	     suggestionItems="{path:'PostalCode>/results'}" -->
								<!--	     value="{ChangeOverModel>/CorrPostalCode}" >-->
								<!--	<suggestionItems>-->
								<!--		<core:Item text="{PostalCode>PostCode}" key="{PostalCode>PostCode}"/>-->
								<!--	</suggestionItems>-->
								<!--</Input>-->
								<Input id="apostInputChange" type="Number" value="{ChangeOverModel>/CorrPostalCode}" class="sapUiSmallMarginBottom" placeholder="Enter Postal Code"/>
							</html:div>
							<html:div class="col-md-6">
								<Label text="Landmark" />
								<Input id="alandInputChange" required="true" value="{ChangeOverModel>/CorrLandmark}" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Landmark"/>
							</html:div>
						</html:div>
						<html:div class="row ebilling">
							<html:div class="col-xs-12">
								<html:h5>E-Billing
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-3">
								<RadioButton id="RBGRP2_1Change" groupName="GroupB" text="Yes" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton id="RBGRP2_2Change" groupName="GroupB" text="No"/>
							</html:div>
							<html:div class="col-md-6 col-sm-12 col-xs-12">
								<html:h6>We promote granulation to avoid waste of papers</html:h6>
							</html:div>
						</html:div>
						<html:div class="row choosemtr">
							<html:div class="col-xs-12">
								<html:h5>I Choose Meter*
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-3">
								<RadioButton id="RBGRP3_1Change" groupName="GroupC" text="Tata Power Meter" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton id="RBGRP3_2Change" groupName="GroupC" text="Purchase Own Meter"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton id="RBGRP3_3Change" groupName="GroupC" text="Existing Utility Meter"/>
							</html:div>
						</html:div>
						<html:div class="row licence">
							<html:div class="col-xs-12">
								<html:h5>Other Details as per your Electricity Bill
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row above">
							<html:div class="col-xs-12">
								<html:h5>Above Flood Level
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-3">
								<RadioButton id="RBFlood1Change" groupName="GroupE" text="Yes" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton id="RBFlood2Change" groupName="GroupE" text="No"/>
							</html:div>
						</html:div>
						<html:div class="row lec">
							<html:div class="col-xs-12">
								<html:h5>Details
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-sm-4">
								<Label text="Existing Utility" />
								    <ActionSelect id="IdExistUtil"  selectedKey="{ChangeOverModel>/ExistingUtility}" width="100%" change="onExistUtilityChange" items="{ path: 'ExistUtility>/results' }">
										<items>
												<core:Item key="{ExistUtility>DomvalueL}" text="{ExistUtility>Ddtext}"/>
										</items>
									</ActionSelect>
								<!--<Input id="lecnameInput" value="{ChangeOverModel>/LecName}" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Existing Utility"/>-->
							</html:div>
							<html:div class="col-sm-4">
								<Label text="Existing Utility Number" required="true" />
								<Input id="seciInputChange" type="Text" value="{ChangeOverModel>/ExistingUtilityNo}" class="sapUiSmallMarginBottom" placeholder="Enter Existing Utility Number"/>
							</html:div>
						
							<html:div class="col-sm-4">
								<Label text="Meter No." required="true" />
								<Input id="galaInputChange" type="Text" value="{ChangeOverModel>/MeterNo}" class="sapUiSmallMarginBottom" placeholder="Enter Meter No."/>
							</html:div>
							</html:div>
						<html:div class="row">
							<html:div class="col-sm-4">
								<Label text="Purpose of Supply" labelFor="IdPurposeSuply"/>
								    <Select id="IdPurposeSuply"  selectedKey="{ChangeOverModel>/PurposeSupply}" change="onSupplyPurposeChange" items="{ path: 'SupplyPurpose>/results' }">
										<items>
											<core:Item key="{SupplyPurpose>DomvalueL}" text="{SupplyPurpose>Ddtext}"/>
										</items>
									</Select>
								<!--<Input id="bnameInput" type="Text" value="{ChangeOverModel>/BuildName2}" class="sapUiSmallMarginBottom" placeholder="Enter Purpose of Supply"/>-->
							</html:div>
						
							<html:div class="col-sm-4">
								<Label text="Category Of Supply" />
								<Select id="IdSuplyCat"  selectedKey="{ChangeOverModel>/SupplyCategory}" change="onSupplyCatChange" items="{ path: 'SupplyCategory>/results' }">
										<items>
											<core:Item key="{SupplyCategory>DomvalueL}" text="{SupplyCategory>Ddtext}"/>
										</items>
									</Select>
								<!--<Input id="socInput" type="Text" value="{ChangeOverModel>/Society2}" class="sapUiSmallMarginBottom" placeholder="Enter Category Of Supply"/>-->
							</html:div>
							<html:div class="col-sm-4">
								<Label text="Connected Load(kW)" required="true" />
								<Input id="streeInputChange" type="Text" value="{ChangeOverModel>/ConnLoad}" class="sapUiSmallMarginBottom" placeholder="Enter Connected Load(kW)"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-sm-4">
								<Label text="Bill Amount" />
								<Input id="banInputChange" type="Text" value="{ChangeOverModel>/BillAmount}" class="sapUiSmallMarginBottom" placeholder="Enter Bill Amount"/>
							</html:div>
							<html:div class="col-sm-4">
								<Label text="Contract Demand(kVA)" />
								<Input id="placInputChange" type="Text" value="{ChangeOverModel>/ContractDmd}" class="sapUiSmallMarginBottom" placeholder="Enter Contract Demand(kVA)"/>
							</html:div>
						
							<html:div class="col-sm-4">
								<Label text="Comsumption" />
								<Input id="psInputChange" type="number" value="{ChangeOverModel>/Consumption}" class="sapUiSmallMarginBottom" placeholder="Enter Comsumption"/>
							</html:div>
							</html:div>
						<html:div class="row">
							<html:div class="col-sm-4">
								<Label text="Type Of Supply" required="true" />
								<Select id="IdTypeSuply"  selectedKey="{ChangeOverModel>/PhaseOfSupply}" change="onSupplyTypeChange" items="{ path: 'SupplyType>/results' }">
										<items>
												<core:Item key="{SupplyType>DomvalueL}" text="{SupplyType>Ddtext}"/>
										</items>
									</Select>
								<!--<Input id="lmInput" type="Text" value="{ChangeOverModel>/LandMark2}" class="sapUiSmallMarginBottom" placeholder="Enter Type Of Supply"/>-->
							</html:div>
						
							<html:div class="col-sm-4">
								<Label text="No. Of Meters" required="true" />
								<Input id="inputemailChange" value="{ChangeOverModel>/NoOfMeters}" placeholder="Enter No. Of Meters"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-3">
								<Button icon="sap-icon://save" text="SAVE AND CONTINUE" press="onSaveNContinue" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
							<html:div class="col-md-3">
								<Button icon="sap-icon://show" text="PREVIEW BASIC DATA" press="onPress" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
							<html:div class="col-md-3">
								<Button icon="sap-icon://save" text="SAVE IN DRAFT" press="onSaveInDraft" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
							<html:div class="col-md-3">
								<Button icon="sap-icon://arrow-left" text="BACK TO REGISTRATION PAGE" press="onPress" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
						</html:div>
					</html:div>
				</html:div>
			</Panel>
		</content>
	</Panel>
</core:FragmentDefinition>