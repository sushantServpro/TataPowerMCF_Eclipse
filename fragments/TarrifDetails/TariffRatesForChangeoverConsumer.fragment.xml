                </table:template>
            </table:Column>
            <table:Column id="col041">
                <table:multiLabels>
                    <Label text="Fixed Charge / Demand Charge  (₹)" design="Bold" wrapping="true" textAlign="Left"/>
                    <Label text="Rate" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
