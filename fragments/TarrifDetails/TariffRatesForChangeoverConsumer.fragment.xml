<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" xmlns:table="sap.ui.table" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel >
		<content>
			<Panel id="pnl_25">
			<html:div class="ebiregister">
					<html:h3 class="text-center">Tariff for Changeover Consumers on R-Infra Wires (FY19) w.e.f. 1st April 2018</html:h3>
				</html:div>
				<VBox>
				<html:div class="container-fluid">
					<html:div class="row view_table">
						<table:Table title="Low Tension categories"
						noDataText="No data to display" id="tableTariffDisp" 
						enableBusyIndicator="true" showNoData="true" 
						enableGrouping= "true" alternateRowColors="true"
						rows="{/TariffChangeoverSet}"
                       
						visibleRowCount="6" selectionMode="None"
						width="auto" class="sapUiResponsiveMargin">
		    <table:columns>
		    <table:Column id="IdCol01" headerSpan="1" colspan="1" >
		    <Label text="Consumer Category" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
		    <table:template>
                    <Text text="{TensionSubcategoryLvl1}"/>
                </table:template>
		    </table:Column>
		    <table:Column id="IdCol02" headerSpan="1" colspan="1">
		    <Label text="Consumer Sub Category" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
		    <table:template>
                    <Text text="{TensionSubcategoryLvl2} {TensionSubcategoryLvl3}"/>
                </table:template>
		    </table:Column>
            <table:Column id="IdCol03"  headerSpan="2" colspan="2">
                <table:multiLabels >
                    <Label text="Fixed Charge / Demand Charge  (₹)" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="UOM (Unit of Measurement)" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{FixedChargeUnit}"/>
                </table:template>
            </table:Column>
            <table:Column id="col04"  headerSpan="1" colspan="1">
                <table:multiLabels>
                    <Label text="Fixed Charge / Demand Charge  (₹)" design="Bold" wrapping="true" textAlign="Left"/>
                    <Label text="Rate" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{FixedChargeRate}"/>
                </table:template>
             </table:Column>
            <table:Column id="IdCol05" headerSpan="1" colspan="1">
                <table:multiLabels >
                    <Label text="Energy Charges" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="₹/kWh" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{EnergyCharges}"/>
                </table:template>
            </table:Column> 
            <table:Column id="IdCol06" headerSpan="1" colspan="1">
                <table:multiLabels >
                    <Label text="Net Energy Charges*" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="₹/kWh" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{NetEnergyCharges}"/>
                </table:template>
            </table:Column>
            <table:Column id="IdCol07" headerSpan="1" colspan="1">
                <table:multiLabels >
                    <Label text="Wheeling Charges Payable to R-Infra" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="₹/kWh" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{WheelingCharges}"/>
                </table:template>
            </table:Column>
            <table:Column id="IdCol08" headerSpan="1" colspan="1">
                <table:multiLabels >
                    <Label text="Cross Subsidy Surcharge Payable to R-Infra" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="₹/kWh" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{CrossSubsidySurcharge}"/>
                </table:template>
            </table:Column>
            
            <table:Column id="IdCol09" headerSpan="1" colspan="1">
                <table:multiLabels >
                    <Label text="Regulatory Asset Charges payable to R-Infra" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="₹/kWh" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{RegulatoryAssetCharges}"/>
                </table:template>
            </table:Column>
            
            <table:Column id="IdCol10" headerSpan="1" colspan="1">
                <table:multiLabels >
                    <Label text="Tata Power Net Tariff(Current Year)" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="₹/kWh" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{NetTariffHigh}"/>
                </table:template>
            </table:Column>
            
            <table:Column id="IdCol11" headerSpan="1" colspan="1">
                <table:multiLabels >
                    <Label text="Tata Power Net Tariff (Previous Year)" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="₹/kWh" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{NetTariffLow}"/>
                </table:template>
            </table:Column>
            
            <table:Column id="IdCol12" headerSpan="1" colspan="1">
                <table:multiLabels >
                    <Label text="Difference (Current - Previous Year)" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="₹/kWh" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
                    <Text text="{NetTariffDiff}"/>
                </table:template>
                </table:Column>
                </table:columns>
            </table:Table>
	    </html:div>
	 </html:div>
		
				<html:div class="container-fluid">
					<html:div class="row view_table">
						<table:Table title="High Tension Categories"
						noDataText="No data to display" id="tableTariffHighTen" 
						enableBusyIndicator="true" showNoData="true"
						enableGrouping= "true" alternateRowColors="true"
						rows="{/TariffChangeoverSet}" 
						visibleRowCount="6" selectionMode="None"
						width="auto" class="sapUiResponsiveMargin">
		    <table:columns>
		    <table:Column id="IdCol011" headerSpan="1" colspan="1">
		    <Label text="Consumer Category" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
		    <table:template>
                    <Text text="{TensionSubcategoryLvl1}"/>
                </table:template>
		    </table:Column>
		    <table:Column id="IdCol021" headerSpan="1" colspan="1">
		    <Label text="Consumer Sub Category" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
		    <table:template>
                    <Text text="{TensionSubcategoryLvl2} {TensionSubcategoryLvl3}"/>
                </table:template>
		    </table:Column>
            <table:Column id="IdCol031" headerSpan="2" colspan="2">
                <table:multiLabels >
                    <Label text="Fixed Charge / Demand Charge  (₹)" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                    <Label text="UOM (Unit of Measurement)" design="Bold" wrapping="true" textAlign="Left" width="100%"/>
                </table:multiLabels>
                <table:template>
