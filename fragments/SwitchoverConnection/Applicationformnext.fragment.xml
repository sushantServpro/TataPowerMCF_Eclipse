<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel >
		<content>
			<Panel >
				<html:h5>Switch Over</html:h5>
				<html:div class="apply">
					<html:div class="row">
						<html:div class=" col-md-2 tata1">
							<html:image src="images/tata_logo_white.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
						<html:div class=" col-md-1 pull-right tata2">
							<html:image src="images/tata_emblem_white.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
					</html:div>
				</html:div>
				<html:h6 class="indicate">*Indicates Mandatory Fields</html:h6>
				<html:div class="detailrequire">
					<html:div class="detailspremises">
						<html:h5 class="indicate2">Switch Over</html:h5>
						<html:hr/>
					</html:div>
					<html:div class="boxpad" id="nextpage">
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Bussiness Partner" labelFor="input-a" required="true"/>
								<Input id="bussinessPartnerInput" type="Text" required='true' class="sapUiSmallMarginBottom" placeholder="Name" value="{ConsumerDetails>/NameBill}"/>
							</html:div>
							<html:div class="col-md-6">
									<Label text="Consumer Number" labelFor="input-a" required="true"/>
								<Input id="consumerNoInput" type="Text" required='true' class="sapUiSmallMarginBottom" placeholder="Consumer Number" value="{ConsumerDetails>/ApplName}"/>
							</html:div>
						</html:div>
						
						
						<html:div class="row buildflat">
							<html:div class="col-md-6">
								<Label text="Application Number" labelFor="input-a" required="true"/>
								<Input id="applnoInput" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="Application" value="{ConsumerDetails>/FlatNo}"/>
							</html:div>
							<html:div class="col-md-6">
								<Label text="Application Charges" labelFor="input-a" required="true"/>
								<Input id="applChargesInput" type="Text" class="sapUiSmallMarginBottom" placeholder="Application Charges" value="{ConsumerDetails>/PlotNo}"/>
							</html:div>
						</html:div>
						
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Service Connection Charges" labelFor="input-a" required="true"/>
								<Input id="servConnChargInput" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Sercvice Connection charges" value="{ConsumerDetails>/Society}"/>
							</html:div>
							<html:div class="col-md-6">
								<Label text="Security Deposite" labelFor="input-a" required="true"/>
								<Input id="secDepInput" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="Enter Security Deposite" value="{ConsumerDetails>/Street}"/>
							</html:div>
						</html:div>
						
						<html:div class="row">
					   		<html:div class="col-md-12">
					   			<html:p >Note : EMI Of 24 months will be applicable for paying the Service Connection Charge which will be billed in your Power Supply bill once the switch over is completed. </html:p>
						</html:div> </html:div>
						<html:div class="row">
							<html:div class="col-md-6">
								<Label text="Document Type" labelFor="input-a" required="true"/>
								<Select class="sapUiSmallMarginBottom" width="100%" id="docType"
									forceSelection="false"
									selectedKey="{/ProductCollection/0/ProductId}"
									items="{
										path: 'SupplyCatSerachdata>/results',
										sorter: { path: '' }
									}">
									<core:Item key="{SupplyCatSerachdata>DomvalueL}" text="{SupplyCatSerachdata>Ddtext}" />
								</Select>
							</html:div>
							<html:div class="col-md-6">
								<Label text="Document Name" labelFor="input-a" required="true"/>
								<Input id="docNameInput" required="true" type="Text" enabled="false" class="sapUiSmallMarginBottom" placeholder="Enter Security Deposite" value="{ConsumerDetails>/Street}"/>
							</html:div>
						</html:div>
							<html:div class="row">
					   		<html:div class="col-md-12">
					   			<html:p >Download and print duly filled form and EMI letter.</html:p>
					   			<html:p >Kindly upload the duly signed form and EMI letter alog with required document.</html:p>
				   				<html:p >All Documents should be self-attached.</html:p>
			   					<html:p >Select document type to uploaded and then Browse/Choose file and attach.</html:p>
			   					<html:p >*Note : Please not the application will be proccessed only if the application form, EMI Letter &amp; Requested docuuments (As per checklist above) is uploaded </html:p>
							</html:div> </html:div>
						<html:div class="row">
							<html:div class="col-md-3">
									<u:FileUploader
												id="fileUploader"
												name="myFileUpload"
												uploadUrl="upload/"
												tooltip="Upload your file to the local server"
												uploadComplete="handleUploadComplete"
												change="handleValueChange"
												typeMissmatch="handleTypeMissmatch"
												style="Emphasized"
												fileType="txt,jpg"
												placeholder="Choose a file for Upload...">
													<u:parameters>
														<u:FileUploaderParameter
															name="param1"
															value="Foo"/>
														<u:FileUploaderParameter
															name="param2"
															value="bar"/>
													</u:parameters>
													<u:headerParameters>
														<u:FileUploaderParameter
															name="headerParam1"
															value="Foo"/>
														<u:FileUploaderParameter
															name="headerParam2"
															value="bar"/>
													</u:headerParameters>
										</u:FileUploader>
						</html:div>
						<html:div class="col-md-4">	
							<Button icon="sap-icon://attachment" text="Attach" id="btnupload" press="onUpload" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
						</html:div>	
						</html:div>
						<html:div class="row">
							<html:div class="col-md-3">
								<Button icon="sap-icon://save" text="Submit" id="btnSaveAsDraft" press="onSaveAsDraft" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
							
						<!--	<html:div class="col-md-3">
								<Button icon="sap-icon://save" text="SAVE IN DRAFT" press="onPress" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
							<html:div class="col-md-3">
								<Button icon="sap-icon://arrow-left" text="BACK TO REGISTRATION PAGE" press="onPress" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>-->
						</html:div>
						
					</html:div>
				</html:div>
			</Panel>
		</content>
	</Panel>
</core:FragmentDefinition>