<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel >
		<content>
			<Panel id="pnl_08">
				<html:h5>Apply Online</html:h5>
				<html:div class="apply">
					<html:div class="row">
						<html:div class=" col-md-2 tata1">
							<html:image src="images/tata_logo_white.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
						<html:div class=" col-md-1 pull-right tata2">
							<html:image src="images/tata_emblem_white.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
					</html:div>
				</html:div>
				<html:h6 class="indicate">*Indicates Mandatory Fields</html:h6>
				<html:div class="detailrequire">
					<html:div class="detailspremises">
						<html:h5 class="indicate2"></html:h5>
						<html:hr/>
					</html:div>
					<html:div class="boxpad">
						<html:div class="row">
							<html:div class=" col-md-2">
								<RadioButton groupName="Groupselect" enabled="false" id="LRRBAPPL1" text="Load Enhancement" select="OnApplicationChange" />
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect"  id="LRRBAPPL2" text="Load Reduction" select="OnApplicationChange" selected="true" />
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect" enabled="false" id="LRRBAPPL3" text="Category Change" select="OnApplicationChange" />
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect" enabled="false" id="LRRBAPPL4" text="Meter Shifting" select="OnApplicationChange" />
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect" enabled="false" id="LRRBAPPL5" text="CMD Enhancement" select="OnApplicationChange" />
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect"  enabled="false" id="LRRBAPPL6" text="CMD Enhancement" select="OnApplicationChange" />
							</html:div>
						</html:div>
						
						<html:div class="row sapUiMediumMarginTop">
							<Label class="col-md-4" text="Contract Acct:" labelFor="input-a"/>
							<html:div class="col-md-4">
								<!--<Input id="contractacctInput" required="true" type="Text" class="sapUiSmallMarginBottom" showValueHelp="true" valueHelpRequest="ValueHelpAcct" />-->
								<Input id="LRcontractacctInput" required="true" type="Text" class="sapUiSmallMarginBottom" showValueHelp="true" valueHelpRequest="ValueHelpAcctsearch" 
								 />
								
								<!--<Input id="TarrifId" showSuggestion="true" showValueHelp="true"
									     valueHelpRequest="handleValueHelp"
									     suggestionItems="{path:'Tariff>/results'}" 
									     value="{BillCalModel>/Tariff}" >-->
								
								<!--<suggestionItems>
										<core:Item text="{Contract>ContractAccountID}" key="{Contract>ContractAccountID}"/>
								</suggestionItems>-->
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Bussiness partn" enabled="false" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRbussinesspartnInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" value="{AddressDetails>/BpNo}"/>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Type Of Consumer" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRtypeofconInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Business Partner Name" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRbpartnernameInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Flat" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRflatInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" value="{AddressDetails>/DefFlat}"/>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4" text="c/o" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRco" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" value="{AddressDetails>/DefBuilding}"/>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Street 2" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRstreet2Input" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" value="{AddressDetails>/DefSociety}"/>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Street 3" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRstreet3Input" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" value="{AddressDetails>/DefLane}"/>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4"  text="City" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRcityInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" value="{AddressDetails>/DefCity}" />
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4"  text="District" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRdistrictInput" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4"  text="Postal Code" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRpcodeInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" value="{AddressDetails>/DefPostal}"/>
						</html:div></html:div> 
						<html:div class="row">
							<Label class="col-md-4"  text="Land Mark" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRlandmarkInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" value="{AddressDetails>/DefLandmark}"/>
						</html:div></html:div> 
						<html:div class="row">
							<Label class="col-md-4" required="true" text="Mobile Phone" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRmphone" maxLength="10" required="true" type="Number" class="sapUiSmallMarginBottom"/>
						</html:div></html:div> 
						<html:div class="row">
							<Label class="col-md-4"  text="E-Mail Address" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRemailInput" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="(Email ID is required for speedy communication)"/>
						</html:div></html:div>
							<html:div class="row">
							<Label class="col-md-4"  text="Meter No." labelFor="input-a"/>
							<html:div class="col-md-4"><Select>
										<core:Item key="100" text="100"/>
										<core:Item key="200" text="200"/>
										<core:Item key="300" text="300"/>
									</Select>
						</html:div></html:div>
						<html:div class="row">
							<Label class="col-md-4"  text="Installation" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRinstallationInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
							<html:div class="row">
							<Label class="col-md-4"  text="Type Of Supply" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRtypeofsupplyInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						
						<html:div class="row">
							<Label class="col-md-4"  text="Rate Category" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRratecategoryInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						
						<html:div class="row">
							<Label class="col-md-4"  text="Existing Con_Load(KW)" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRexistingconloadInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="0.0000000"/>
						</html:div></html:div>
						
					<html:div class="row">
							<Label class="col-md-4"  text="Existing CMD(KVA)" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRexistingcmdInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="0.0000000"/>
						</html:div></html:div>
						
							<html:div class="row">
							<Label class="col-md-4"  text="Required/Additional Con_Load(KW)" labelFor="input-a" required="true"/>
							<html:div class="col-md-4"><Input id="LRreqconloadInput" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="0.0000000"/>
						</html:div></html:div>
						
					<html:div class="row">
							<Label class="col-md-4"  text="Required/Additional CMD(KVA)" labelFor="input-a" required="true"/>
							<html:div class="col-md-4"><Input id="LRreqcmdInput" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="0.0000000"/>
						</html:div></html:div>
						
							<html:div class="row">
							<Label class="col-md-4"  text="Required Rate Category" labelFor="input-a" />
							<html:div class="col-md-4">
								
									
								<Select class="sapUiSmallMarginBottom" width="100%" id="LRreqratecatInput"
									forceSelection="false"
									selectedKey=""
									items="{
										path: 'SupplyCatSerachdata>/results',
										sorter: { path: '' }
									}">
									<core:Item key="{SupplyCatSerachdata>DomvalueL}" text="{SupplyCatSerachdata>Ddtext}" />
								</Select>
								<!--<Input id="reqratecatInput" required="true" type="Text" class="sapUiSmallMarginBottom"/>-->
						</html:div></html:div>
						
					
						
							<html:div class="row">
							<Label class="col-md-4"  text="New Connected Load (kW)"  labelFor="input-a" />
							<html:div class="col-md-4"><Input id="LRnewconnectedloadkwInput" enabled="false" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						
							<html:div class="row">
							<Label class="col-md-4"  text="New Contract Demand (kVA)"  labelFor="input-a" />
							<html:div class="col-md-4"><Input id="LRnewcontrctdemandInput" enabled="false"  type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						
							<html:div class="row">
							<html:p class="col-md-12 text-danger">New Connected Load(kW) / New Contracted Demand (KVA) will consider for billing purpose.
						</html:p></html:div>
					
						<html:div class="row">
							<Label class="col-md-4"  text="Type of premises" labelFor="input-a" />
							<html:div class="col-md-2">
								<RadioButton id="LRRB11" groupName="GroupA" text="Owned" selected="true"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="LRRB22" groupName="GroupA" text="Rented"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="LRRB33" groupName="GroupA" text="Lease"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="LRRB44" groupName="GroupA" text="Other (Specify)"/>
							</html:div>
						</html:div>
						
						<html:div class="row powerreq">
							<html:div class="col-xs-12">
								<html:h5>Power Supply Requirement
								</html:h5>
							</html:div>
						</html:div>
						
						<html:div class="row">
							<html:div class="col-md-offset-2 col-md-3">
								<CheckBox id="LRLowVoltSingle" text="Low Voltage Single Phase" selected="false"/>
							</html:div>
							<html:div class="col-md-3">
								<CheckBox id="LRLowVoltThree" text="Low Voltage Three Phase" selected="false"/>
							</html:div>
								<html:div class="col-md-3">
								<CheckBox id="LRHighVolt" text="High Voltage" selected="false"/>
							</html:div>
						</html:div>
						
						<html:div class="row powerreq">
							<html:div class="col-xs-12">
								<html:h5>Load Details
								</html:h5>
							</html:div>
						</html:div>
						
						<html:div class="row">
							<html:div class="col-md-offset-2 col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID1" text="LED lights/Tube lights/Bulbs" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID2" text="Fans" selected="true"/>
							</html:div>
								<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID3" text="Geysers" selected="true"/>
							</html:div>
						</html:div>
						
						<html:div class="row">
							<html:div class="col-md-offset-2 col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID4" text="Oven/Micro Wave" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID5" text="Chiller/Air Conditioners" selected="true"/>
							</html:div>
								<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID6" text="Refrigerator" selected="true"/>
							</html:div>
						</html:div>
						
						<html:div class="row">
							<html:div class="col-md-offset-2 col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID7" text="Television" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID8" text="Machinery/Apparatus" selected="true"/>
							</html:div>
								<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="LRRBGRP_ID9" text="Miscellaneous" selected="true"/>
							</html:div>
						</html:div>
						
						<html:div class="row">
							<Label class="col-md-4"  text="No of Points"  labelFor="input-a" required="true" />
							<html:div class="col-md-4"><Input id="LRpointInput"   type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
							<html:div class="row">
							<Label class="col-md-4"  text="W/Point" labelFor="input-a" required="true" />
							<html:div class="col-md-4"><Input id="LRwattInput"   type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						<html:div class="row">
							<html:div class="col-md-1">
								<Button  text="Add Load Details" press="onAddLoadDetails" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
							<html:div class="col-md-1">
								<Button  text="Remove Load Details" press="onRemoveLoadDetails" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
						</html:div>	
			   			<html:div class="row">
			   				<Table id="LRApplyOlLoadTab" growing="true"
			            noDataText="{NewConnectionView>/tableNoDataText}"
			            growingScrollToLoad="true"
			            mode="SingleSelectLeft"
                        items="{oLoadTableModel}">
							<columns>
								<Column minScreenWidth="Desktop">
									<Text text="Apertures"/>
								</Column>
								<Column minScreenWidth="Desktop" demandPopin="true">
									<Text text="Number Of Point"/>
								</Column>
								<Column minScreenWidth="Desktop" demandPopin="true">
									<Text text="Wattage Per Point"/>
								</Column>
								<Column minScreenWidth="Desktop" demandPopin="true">
									<Text text="Total Wattage"/>
								</Column>
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<ObjectIdentifier title="{Apertures}"/>
										<Text text="{NoPoint}"/>
										<Text text="{WattPerPoint}"/>
										<Text text="{TotWatt}"/>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
			   				
								
						</html:div>
						
						<html:div class="row powerreq">
							<html:div class="col-xs-12">
								<html:h5>Licenced Electrical Contract Details
								</html:h5>
							</html:div>
						</html:div>
						
						<html:div class="row">
							<Label class="col-md-4"  text="Name" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRnameInputId" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						
						<html:div class="row">
							<Label class="col-md-4"  text="Licence No" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRliencenoInputId" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
					
							<html:div class="row">
							<Label class="col-md-4"  text="Mobile No" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRmobilenoInputId" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
						
						<html:div class="row">
							<Label class="col-md-4"  text="E-Mail Address" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="LRemailidInputId" required="true" type="Text" class="sapUiSmallMarginBottom"/>
						</html:div></html:div>
					
						<html:div class="row">
							<html:div class="col-md-3">
								<Button icon="sap-icon://save" text="Submit" press="onSaveMeterData" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
						</html:div>	
					</html:div>
				</html:div>
			</Panel>
		</content>
	</Panel>
</core:FragmentDefinition>